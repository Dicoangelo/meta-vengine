          {
            "type": "command",
            "command": "echo '{\"ts\":'$(date +%s)',\"tool\":\"WebSearch\",\"source\":\"hook\"}' >> ~/.claude/data/tool-usage.jsonl",
            "timeout": 3
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo '\n══════════════════════════════════════' >> ~/.claude/activity.log && echo \"SESSION $(date '+%Y-%m-%d %H:%M:%S') PWD:$(pwd)\" >> ~/.claude/activity.log && echo '{\"ts\":'$(date +%s)',\"event\":\"session_start\",\"pwd\":\"'$(pwd)'\"}' >> ~/.claude/data/session-events.jsonl && node ~/.claude/kernel/activity-tracker.js start 2>/dev/null &",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo \"SESSION END $(date '+%H:%M:%S')\" >> ~/.claude/activity.log && echo '{\"ts\":'$(date +%s)',\"event\":\"session_end\",\"pwd\":\"'$(pwd)'\"}' >> ~/.claude/data/session-events.jsonl",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "node ~/.claude/kernel/activity-tracker.js end 2>/dev/null || true",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "bash ~/.claude/scripts/research-auto-capture.sh 2>/dev/null || true",
            "timeout": 30
          },
          {
            "type": "command",
            "command": "python3 ~/.claude/scripts/meta-analyzer.py analyze --quiet 2>/dev/null || true",
            "timeout": 60
          },
          {
            "type": "command",
            "command": "bash ~/.claude/scripts/kernel-feeder.sh 2>/dev/null || true",
            "timeout": 30
          }
        ]
      }
    ]
  }
}
